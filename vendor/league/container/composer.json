{
    "name": "league/container",
    "description": "A fast and intuitive dependency injection container.",
    "keywords": [
        "league",
        "container",
        "dependency",
        "injection",
        "di"
    ],
    "homepage": "https://github.com/thephpleague/container",
    "license": "MIT",
    "authors": [
        {
            "name": "Phil Bennett",
            "email": "philipobenito@gmail.com",
            "homepage": "http://philipobenito.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "replace": {
        "orno/di": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "League\\Container\\": "src"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/Asset/functions.php"
        ],
        "psr-4": {
            "League\\Container\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev",
            "dev-1.x": "1.3-dev"
        }
    }
}
